# Server Configuration
PORT=5000
NODE_ENV=development

# API Keys (Optional - some APIs don't need keys)
AMBEE_API_KEY=your_ambee_api_key_here_optional

# CORS Settings
ALLOWED_ORIGINS=http://localhost:3000,http://your-domain.com

# Cache Settings (in seconds)
CACHE_DURATION=30

# API Rate Limits
OPENSKY_RATE_LIMIT=10000
USGS_RATE_LIMIT=60000
EONET_RATE_LIMIT=60000